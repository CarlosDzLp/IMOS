<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage 
    xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    xmlns:local="clr-namespace:IMOS.Views.Principal;assembly=IMOS"
    x:Class="IMOS.Views.Principal.TabVentasPage"
    Title="{Binding TitleNombre}">
    <NavigationPage.TitleView>
        <StackLayout
            Orientation="Vertical">
            <Label
                TextColor="White"
                Text="{Binding TitleNombre}"/>
            <Label
                TextColor="White"
                Text="{Binding Credito}"/>
        </StackLayout>
    </NavigationPage.TitleView>
</TabbedPage>
